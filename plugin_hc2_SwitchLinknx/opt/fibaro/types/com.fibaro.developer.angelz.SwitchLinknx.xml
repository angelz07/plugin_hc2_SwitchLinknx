<?xml version="1.0" encoding="utf-8"?>
<device type="com.fibaro.developer.angelz.SwitchLinknx" baseType="com.fibaro.binarySwitch">
  <interfaces>
        
        <interface name="energy"/>
    </interfaces>
  <properties>
    <property name="ip_nodejs" type="string"/>
    <property name="port_nodejs" type="string"/>
    <property name="id_linknx_cmd" type="string"/>
    <property name="id_linknx_status" type="string"/>
